{
  "name": "imagingsearch",
  "version": "1.0.4",
  "description": "Hybrid imaging search with DICOM metadata filtering and vector-based semantic search",
  "main": "server.js",
  "private": true,
  "scripts": {
    "dev": "concurrently \"react-scripts start\" \"node server.js\"",
    "start": "npm run build && node server.js",
    "build": "react-scripts build && cp -R build/* public/",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "release": "npm run build && release-it --ci"
  },
  "keywords": [],
  "author": "Jason Klotzer",
  "license": "Apache-2.0",
  "proxy": "http://localhost:5000",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@google-cloud/bigquery": "^8.0.0",
    "@google/genai": "^0.13.0",
    "@mui/material": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "concurrently": "^8.2.0",
    "jest": "^29.7.0",
    "react-scripts": "5.0.1",
    "release-it": "^19.0.2",
    "supertest": "^6.3.3",
    "web-vitals": "^2.1.4"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "hooks": {
      "after:release": [
        "docker build . --tag jasonklotzer/imagingsearch:latest --tag jasonklotzer/imagingsearch:${version} --no-cache",
        "docker push jasonklotzer/imagingsearch --all-tags",
        "gcloud run deploy imagingsearch --set-env-vars=\"GCP_PROJECT_ID=data-axe-350723\" --image=jasonklotzer/imagingsearch:latest --region=us-central1 --project=data-axe-350723",
        "gcloud run services update-traffic imagingsearch --to-latest"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
